{
  "name": "horat1us/yii2-environment-config",
  "description": "Yii2 environment-config integration",
  "type": "library",
  "require": {
    "php": ">=7.3",
    "yiisoft/yii2": "^2.0.19",
    "horat1us/environment-config": "^1.4",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Horat1us\\Environment\\Yii2\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Horat1us\\Environment\\Yii2\\Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Alexander Letnikow",
      "email": "reclamme@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "scripts": {
    "lint": "./vendor/bin/phpcs --standard=PSR2 ./src ./tests",
    "phpcbf": "./vendor/bin/phpcbf --standard=PSR2 ./src ./tests",
    "test": "./vendor/bin/phpunit"
  }
}
